{
  "rust-analyzer.linkedProjects": [
    "./Cargo.toml",
    "./crates/*",
    "./migration/Cargo.toml"
  ],
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  }
}
